{
  "name": "abatar",
  "version": "0.2.3",
  "description": "A/B testing minion",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-node && npm run test-browsers",
    "test-node": "tape test/*.js",
    "test-browsers": "zuul -- test/*.js",
    "bundle": "browserify index.js -o bundle.js -s AB"
  },
  "author": "Danny Coates",
  "license": "MPL 2.0",
  "dependencies": {
    "browserify": "8.0.3",
    "sha1": "1.1.0"
  },
  "devDependencies": {
    "tape": "3.0.3",
    "zuul": "1.16.4"
  }
}
